    "url": "https://github.com/KaushikPrakash/missing-kittens/issues"
  },
  "homepage": "https://github.com/KaushikPrakash/missing-kittens#readme",
  "dependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.16",
    "@types/node": "^18.11.18",
    "body-parser": "^1.20.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^6.0.1",
    "rimraf": "^4.1.2",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
